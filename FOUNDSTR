#define TASK  "4"
#include <bits/stdc++.h> 
#define             pb  push_back  
#define             ll  long long 
#define             se  second 
#define             fi  first
#define             el  "\n"
#define      alla(a,n)  a,a+n
#define         all(v)  v.begin(),v.end()
#define     FOR(i,a,b)  for(ll i=a;i<=b;i++)
#define    FORD(i,a,b)  for(ll i=a;i>=b;i--)
using namespace std;

const long long inf = 1e18;
const ll N = 1e6 + 5 , N1 = 1e3 + 5 , MOD = 1e9 +7;
const double PI = asin(1) * 2;

ll gcd(ll a,ll b){return __gcd(a,b);}
ll bp(ll a){return a*a;}
ll lcm(ll a,ll b){return a*b/gcd(a,b);}

ll dx[]={0,0,-1,1};
ll dy[]={1,-1,0,0};


ll n,m,v;
ll t[1e6+5][26],f[1e6+5];

void add(string s)
{
    ll r=1;
    FOR(i,0,s.size()-1)
    {
        ll j=s[i]-'a';
        if(t[r][j]==-1)
        {
            v++;
            t[r][j]=v;
        }
        r=t[r][j];
    }
    f[r]=1;
}

void find(string a)
{
    ll r=1;
    FOR(i,0,s.size()-1)
    {
        ll j=s[i]-'a';
        if(t[r][j]==-1) return 0;
        r=t[r][j];
    }
    return r;
}

void sol()
{
    memset(t,-1,sizeof(t));
    cin>>n;
    FOR(i,1,n)
    {
        string s;cin>>s;
        add(s);
    }
    cin>>m;
    FOR(i,1,m)
    {
        string s;
        cin>>s;
        ll u=find(s);
        if(f[u]) cout<<1<<el;
        else cout<<0<<el;
    }
}


    

// "..."

signed main(void)
{
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);srand(time(0)); 
    if(fopen(TASK".inp","r"))
    {
        freopen(TASK".inp" ,"r",stdin) ;
        freopen(TASK".out" ,"w",stdout);
    }
    sol();
    // cerr<<el<<"Time: "<<0.001*clock()<<"s"<<el;return 0;
}
